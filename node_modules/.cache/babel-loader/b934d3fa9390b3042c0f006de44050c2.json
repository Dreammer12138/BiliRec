CA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EACA,QACA,CAAA,CAAA,QAAA,CAAA,OADA,IAEA,CAAA,CAAA,QAAA,CAAA,MAAA,KAAA,CAAA,GAAA,KAAA,GAAA,KAFA;AAHA,WAAA;AAQA,SAVA,MAUA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAFA,WAAA;AAIA;AACA,OApBA;AAqBA,KAzBA;AA0BA,IAAA,cA1BA,4BA0BA;AAAA;;AACA,UAAA,GAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA;AACA,UAAA,CAAA,GAAA,IAAA;AACA,MAAA,CAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,MAAA,EAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,QAAA,CAAA,CAAA,QAAA,CAAA,OAAA,GAAA;AAFA,WAAA;;AAIA,UAAA,CAAA,CAAA,KAAA,CAAA,gBAAA;AACA,SANA,MAOA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAFA,WAAA;AAIA;AACA,OAdA;AAeA;AA5CA;AANA,CAAA","sourcesContent":["<template>\n  <div class=\"listener-item\">\n    <el-col :span=\"16\">\n      <strong class=\"title\">{{ listener.Name }}</strong>\n      <br />\n      <span class=\"liveurl\">{{ listener.LiveURL }}</span>\n    </el-col>\n    <el-col :span=\"8\" v-if=\"btnShow\">\n      <el-button-group>\n        <el-button icon=\"el-icon-edit\"></el-button>\n        <el-button\n          icon=\"el-icon-switch-button\"\n          @click=\"switchy\"\n          :class=\"listener.Status === 0 ? 'text-dinger' : 'text-success'\"\n        ></el-button>\n        <el-button type=\"danger\" icon=\"el-icon-delete\" @click=\"deleteListener\"></el-button>\n      </el-button-group>\n    </el-col>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ListenerItem\",\n  props: {\n    listener: Object,\n    btnShow: Boolean\n  },\n  methods: {\n    switchy() {\n      const API = this.$store.state.API;\n      let v = this;\n      v.$axios.post(API.listener.switch, {\n        LiveURL: v.listener.LiveURL,\n        Status: v.listener.Status === 0 ? 1 : 0\n      }).then((res) => {\n        if (res.data.status) {\n          v.listener.Status = v.listener.Status === 0 ? 1 : 0;\n          this.$notify.success({\n            title: \"成功\",\n            message:\n              \"已将 \" +\n              v.listener.LiveURL +\n              (v.listener.Status === 0 ? \" 关闭\" : \" 启动\"),\n          });\n          \n        } else {\n          this.$notify.error({\n            title: \"错误\",\n            message: res.data.msg,\n          });\n        }\n      });\n    },\n    deleteListener() {\n      const API = this.$store.state.API;\n      let v = this;\n      v.$axios.post(API.listener.delete, this.listener).then(res => {\n        if (res.data.status) {\n          this.$notify.success({\n            title: '成功',\n            message: '已将 ' + v.listener.LiveURL + ' 删除'\n          });\n          v.$emit('reloadListener');\n        }\n        else {\n          this.$notify.error({\n            title: '错误',\n            message: res.data.msg\n          });\n        }\n      });\n    }\n  },\n};\n</script>\n\n<style scoped>\n.listener-item {\n  height: 6vh;\n  border-top: 1px solid #dcdfe6;\n  border-bottom: 1px solid #dcdfe6;\n  border-right: 1px solid #dcdfe6;\n  border-left: 10px solid #409eff;\n  padding: 1rem;\n  margin: 1rem 0;\n}\n.el-col {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  height: 100%;\n}\n.title {\n  font-size: 1.5rem;\n}\n.liveurl {\n  font-size: 0.8rem;\n  color: #aaaaaa;\n}\n.text-dinger {\n  color: #f56c6c;\n}\n.text-success {\n  color: #67c23a;\n}\n.text-warning {\n  color: #e6a23c;\n}\n</style>"],"sourceRoot":"src/components"}]}