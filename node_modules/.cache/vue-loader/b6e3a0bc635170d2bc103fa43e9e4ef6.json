res.data.msg\n          });\n        }\n      });\n    },\n    getSettings() {\n      const API = this.$store.state.API;\n      let v = this;\n      v.$axios.get(API.setting.get).then(res => {\n        for (let setting in res.data) {\n          v.settings[res.data[setting].key] = res.data[setting].value;\n        }\n      })\n    }\n  },\n  mounted() {\n    this.getSettings();\n  }\n};\n</script>\n\n<style scoped>\n.el-form-item {\n  text-align: left;\n}\n</style>"]}]}